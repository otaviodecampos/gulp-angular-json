(function() {
    'use strict'

    angular.module('<%= module %>')
        .config(State);

    /* @ngInject */
    function State($urlRouterProvider, $stateProvider, APP) {

        $urlRouterProvider.otherwise(APP.defaultUrl);

        $stateProvider
            .state('<%= name %>', <%= JSON.stringify(content, null, 10) %>);

    };

})();